#include <bits/stdc++.h>
using namespace std;

struct Node {
    int data;
    Node *next;

    // Constructor for Node
    Node(int data1, Node *next1) {
        data = data1;
        next = next1;
    }
};

int main() {
    vector<int> arr = {2, 5, 7, 8};
    Node *y = new Node(arr[0], nullptr);
    cout << y->data << endl;

    // Cleanup the allocated memory
    delete y;
    
    return 0;
}
